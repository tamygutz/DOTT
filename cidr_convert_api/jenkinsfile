pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                checkout([$class: 'GitSCM', branches: [[name: '*/Project']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/tamygutz/DOTT.git']]])
            }
        }
        stage('Build') {
            steps {
                git branch: 'Project', url: 'https://github.com/tamygutz/DOTT.git'
            }
        }
        stage('Test') {
            steps {
                echo "The job have been tested"
            }
        }
        }
}